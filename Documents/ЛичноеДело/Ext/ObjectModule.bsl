	// регистр  ЛичноеДелоЗакрыть
	Если ДелоЗакрыто=Истина Тогда
		Движения.ЛичноеДелоЗакрыть.Записывать = Истина;
	    Движение = Движения.ЛичноеДелоЗакрыть.Добавить();
		Движение.Период = ДатаЗакрытияДела;
		Движение.Организация = Организация;		
		Движение.Заявитель = Заявитель;
		Движение.ЛьготнаяКатегория = ЛьготнаяКатегория;
		Если Льгота Тогда
			 Движение.ВидЛьготы = ВидыЛьготыШапка;
		КонецЕсли;
		Движение.НомерДела = НомерЗаявления;
		Движение.ДатаОткрытияДела = ДатаЗаявления;
		Движение.ДатаЗакрытияДела = ДатаЗакрытияДела;
	КонецЕсли;
	
	Если МногодетнаяСемья Тогда
		Если ДокументыМС.Количество()>0 Тогда
			МногодетнаяСемья();
		КонецЕсли;	
	КонецЕсли;	
	
	ДвиженияПоРегистрамНакопления();	
	КонецПроцедуры	
Процедура КоличествоДетейМС()
	Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ
		|	СУММА(1) КАК КоличествоДетей
		|ИЗ
		|	Документ.ЛичноеДело.ДокументыМС КАК ЛичноеДелоДокументыМС
		|ГДЕ
		|	(ЛичноеДелоДокументыМС.Родство = ЗНАЧЕНИЕ(Справочник.Родство.дочь)
		|			ИЛИ ЛичноеДелоДокументыМС.Родство = ЗНАЧЕНИЕ(Справочник.Родство.сын))
		|	И ЛичноеДелоДокументыМС.Ссылка = &Ссылка";
	
	Запрос.УстановитьПараметр("Ссылка", Ссылка);
	
	РезультатЗапроса = Запрос.Выполнить();
	
	Выборка = РезультатЗапроса.Выбрать();
	
	Если Выборка.Следующий() Тогда
		КоличествоДетейМС =   Выборка.КоличествоДетей;
	КонецЕсли;
	
КонецПроцедуры		
Процедура ПередЗаписью(Отказ, РежимЗаписи, РежимПроведения)
	//НомерЗаявления =ПрефиксацияОбъектовКлиентСервер.УдалитьЛидирующиеНулиИзНомераОбъекта(Номер);
	КоличествоДетейМС();
	Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ
		|	ЛичноеДело.Заявитель КАК Заявитель,
		|	ЛичноеДело.НомерЗаявления КАК НомерЗаявления,
		|	ЛичноеДело.ДатаЗаявления КАК ДатаЗаявления,
		|	ЛичноеДело.Решение КАК Решение
		|ИЗ
		|	Документ.ЛичноеДело КАК ЛичноеДело
