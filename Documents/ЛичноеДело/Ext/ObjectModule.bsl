Процедура ОбработкаЗаполнения(ДанныеЗаполнения, СтандартнаяОбработка)
	
	Если ТипЗнч(ДанныеЗаполнения) = Тип("СправочникСсылка.ФизическиеЛица") Тогда
		// Заполнение шапки
			
			КолСемья = 1; 
			АдресПрописка = ДанныеЗаполнения.АдресПрописка;
			АдресФактическогоПроживания = ДанныеЗаполнения.АдресФактическогоПроживания;
			АдресФактПроживанияИПропискаСовпадают = ДанныеЗаполнения.АдресФактПроживанияИПропискаСовпадают;
			СельскийСовет = ДанныеЗаполнения.СельскийСовет;
			Заявитель = ДанныеЗаполнения.Ссылка;
			НомерТелефона = ДанныеЗаполнения.НомерТелефона;
		Для Каждого ТекСтрокаСоставСемьи Из ДанныеЗаполнения.СоставСемьи Цикл
			НоваяСтрока = СоставСемьи.Добавить();
			НоваяСтрока.Родство = ТекСтрокаСоставСемьи.Родство;
			НоваяСтрока.ФИО = ТекСтрокаСоставСемьи.ФИО;
			КолСемья = КолСемья + 1;
		КонецЦикла;
		Для Каждого ТекСтрока Из ДанныеЗаполнения.ЛьготныеКатегории Цикл
			ЛьготнаяКатегория = ТекСтрока.Категория;
			Группа = ТекСтрока.Группа;
		КонецЦикла;
			 
		    СоставСемьиКоличество = КолСемья;
	КонецЕсли;
	
КонецПроцедуры

Процедура ПроводимКомиссияРегистрСведений();
	// регистр ЛичноеДелоКомиссия
	Движения.ЛичноеДелоКомиссия.Записывать = Истина;
	Движение = Движения.ЛичноеДелоКомиссия.Добавить();
	Движение.Период = Дата;
	Движение.Организация = Организация;
	Движение.ВидЛьготы = ВидыЛьготыШапка;
	Движение.ВидВыплаты = ВидыВыплатШапка;
	Движение.Сотрудник = СпециалистКомиссия;
	Движение.Заявитель = Заявитель;
	Движение.ЛьготнаяКатегория = ЛьготнаяКатегория;
	Движение.СельскийСовет = СельскийСовет;
	Движение.НомерДела = НомерЗаявления;
	Движение.ДатаОткрытияДела = ДатаЗаявления;
	Движение.ПричинаКомиссии = ПричинаКомиссии;
	Движение.ДатаКомиссии = ДатаКомиссии;
	Движение.НомерПротокола = НомерПротокола;
	Движение.ДатаИсполненияРешенияКомиссии = ДатаИсполненияРешенияКомиссии;
	Движение.РешениеКомиссииТекст = РешениеКомиссииТекст;
	Движение.Решение = Решение;
	Если  ЗначениеЗаполнено(Решение) Тогда
		Движение.Закрыто = Истина;
	КонецЕсли;
	Движение.АдресРегистрации = АдресПрописка;
	
КонецПроцедуры	

Процедура ОбработкаПроведения(Отказ, Режим)
	Если ДокПроводить Тогда  
		ДвижениеПоРегистрамСведений();
		Если  ЗначениеЗаполнено(Решение) И ЗначениеЗаполнено(РешениеКомиссииТекст) Тогда
			ПроводимКомиссияРегистрСведений();		
		КонецЕсли;
	
	Иначе
		Если  ЗначениеЗаполнено(Решение) И ЗначениеЗаполнено(РешениеКомиссииТекст) Тогда
			ПроводимКомиссияРегистрСведений();		
		КонецЕсли;	
		Если Решение = Перечисления.Решение.Отказ Тогда
			Если Льгота = Истина Тогда 
				ПроводимОтказЛьгота();
			КонецЕсли;		
		ИначеЕсли (Комиссия = Истина) И (Льгота = Истина) Тогда
			ПроводимКомиссияЛьготы();
			ПроводимКомиссияРегистрСведений();
		
		Иначе	
		Отказ = Истина;
		КонецЕсли;
	КонецЕсли;
	Если ЗначениеЗаполнено(НомерТалона) Тогда
		ЗаписьНомераТалонаВРегистр();
	КонецЕсли;
КонецПроцедуры

	
